<?xml version="1.0"?>
<launch>
    
  <param name="/use_sim_time" value="true" />  

  <node pkg="topic_tools" type="relay" name="multisense_left_relay" args="/multisense_sl/left/image_rect_color_throttled /multisense_sl/left/image_rect" />
  <node pkg="topic_tools" type="relay" name="multisense_points2_relay" args="/multisense_sl/points2_throttled /multisense_sl/points2" />
  
  <node pkg="topic_tools" type="relay" name="atlas_joint_states_relay" args="/atlas/joint_states /test_joint_states" />
  <node pkg="topic_tools" type="relay" name="joint_states_relay" args="/joint_states /test_joint_states" />
  
  <param name="robot_description" command="$(find xacro)/xacro.py '$(find flor_atlas_description)/robots/flor_atlas.urdf.xacro'" />
  
  <node pkg="robot_state_publisher" type="state_publisher" name="atlas_robot_state_publisher" respawn="true" >
    <param name="publish_frequency" type="double" value="50.0" />
    <remap from="joint_states" to="/test_joint_states" />
  </node>
  
  
  
  <!--
  <node pkg="rosbag" type="record" name="calibration_verification_rosbag_recorder" args="-o $(find flor_bag_record)/bags/calibration_verification /multisense_sl/points2_throttled /multisense_sl/left/image_rect_color_throttled /atlas/joint_states /joint_states" />
   -->
  <!--
  rosbag record /multisense_sl/points2_throttled /multisense_sl/left/image_rect_color_throttled /atlas/joint_states /joint_states
  -->
 
</launch>
